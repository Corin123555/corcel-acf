{
  "name": "corin123555/corcel-acf",
  "description": "Advanced Custom Field (ACF) plugin for Corcel",
  "homepage": "http://github.com/corin123555/corcel-acf",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas Bruckmaier"
    },
    {
      "name": "Junior Grossi",
      "email": "juniorgro@gmail.com",
      "homepage": "http://jgrossi.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "jgrossi/corcel": "~3.0",
    "greabock/tentacles": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "orchestra/testbench": "~3.4",
    "orchestra/database": "~3.4"
  },
  "autoload": {
    "psr-4": {
      "Tbruckmaier\\Corcelacf\\": "src/",
      "Tbruckmaier\\Corcelacf\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
        "providers": [
            "Tbruckmaier\\Corcelacf\\ServiceProvider"
        ]
    }
  }
}
